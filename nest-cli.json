{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/alpha/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/alpha/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/alpha",
  "projects": {
    "alpha": {
      "type": "application",
      "root": "apps/alpha",
      "entryFile": "main",
      "sourceRoot": "apps/alpha/src",
      "compilerOptions": {
        "tsConfigPath": "apps/alpha/tsconfig.app.json"
      }
    },
    "beta": {
      "type": "application",
      "root": "apps/beta",
      "entryFile": "main",
      "sourceRoot": "apps/beta/src",
      "compilerOptions": {
        "tsConfigPath": "apps/beta/tsconfig.app.json"
      }
    },
    "gama": {
      "type": "application",
      "root": "apps/gama",
      "entryFile": "main",
      "sourceRoot": "apps/gama/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gama/tsconfig.app.json"
      }
    }
  }
}